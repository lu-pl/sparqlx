# RDF Test Graph - 3 Persons with Key RDF Features
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.org/> .

ex:alice rdf:type foaf:Person ;
         foaf:name "Alice Johnson"@en , "Alice Dupont"@fr ;
         foaf:age "30"^^xsd:int ;
         foaf:birthday "1993-05-15"^^xsd:date ;
         foaf:height "1.75"^^xsd:decimal ;
         foaf:married true ;
         foaf:mbox <mailto:alice@example.org> ;
         foaf:homepage <http://alice.example.org> ;
         ex:skills ( "Python" "RDF" "JavaScript" ) ;
         foaf:knows ex:bob , _:charlie .

ex:bob rdf:type foaf:Person ;
       foaf:name "Bob Smith" ;
       foaf:age 28 ;
       foaf:interest "Music" , "Photography" , "Travel" ;
       foaf:phone _:phoneNumbers ;
       foaf:knows ex:alice .

_:phoneNumbers rdf:type rdf:Alt ;
               rdf:_1 "+1-555-111-2222" ;
               rdf:_2 "+1-555-333-4444" .

_:charlie foaf:name "Charlie Brown" ;
          foaf:age "35"^^xsd:int ;
          foaf:knows [
              foaf:name "David Wilson" ;
              foaf:mbox <mailto:david@example.org> ;
              ex:lastSeen "2023-12-01T14:30:00Z"^^xsd:dateTime
          ] ;
          ex:bio """Charlie is a software engineer
who loves semantic web technologies.""" .

# Reification
ex:alice ex:believes ex:stmt1 .
ex:stmt1 rdf:type rdf:Statement ;
         rdf:subject ex:bob ;
         rdf:predicate foaf:age ;
         rdf:object 28 ;
    ex:confidence "0.95"^^xsd:float .
